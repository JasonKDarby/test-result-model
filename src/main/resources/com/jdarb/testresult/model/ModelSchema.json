{
  "description": "schema for a testresult-model Run",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "startTime": {
      "type": "string",
      "format": "date-time"
    },
    "endTime": {
      "type": "string",
      "format": "date-time"
    },
    "tests": {
      "type": "array",
      "items": { "$ref": "#/definitions/test" }
    },
    "id": {
      "type": "string"
    }
  },
  "required": [
    "name",
    "startTime",
    "endTime",
    "tests",
    "id"
  ],
  "definitions": {
    "test": {
      "properties": {
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "startTime": {
          "type": "string",
          "format": "date-time"
        },
        "endTime": {
          "type": "string",
          "format": "date-time"
        },
        "status": {
          "type": "string"
        },
        "details": {
          "type": "object",
          "properties": {
            "logMessages": {
              "type": "array",
              "items": { "$ref": "#/definitions/logmessage" }
            },
            "childTests": {
              "type": "array",
              "items": { "$ref": "#/definitions/test"}
            }
          },
          "required": [
            "logMessages",
            "childTests"
          ]
        }
      }
    },
    "logmessage": {
      "properties": {
        "text": {
          "type": "string"
        },
        "instant": {
          "type": "string",
          "format": "date-time"
        }
      },
      "required": [
        "text",
        "instant"
      ]
    }
  }
}